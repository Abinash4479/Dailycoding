class multiplication_table {
    void multiplication(int n) {
        for(int i = 1; i <= 10; i++) {
            System.out.format("%d X %d = %d\n", n, i, n * i);
        }
    }
    public static void main(String args[]) {
        multiplication_table table = new multiplication_table();
        table.multiplication(7);
    }
}
