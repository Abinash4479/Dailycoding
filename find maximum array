class maximum_array
{
    public static void main(String args[])
    {
        int [] arr={1,2,3,4,5,6};
        int max=0;
        for(int element:arr)
        {
            if(element>max)
            {
                max=element; 
            }
        }
            System.out.println("maximum is:"+max);
        }
    }

        
